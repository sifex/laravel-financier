{
    "name": "sifex/stripe-connect",
    "description": "A Laravel Stripe Connect Library",
    "keywords": [
        "sifex",
        "stripe-connect"
    ],
    "homepage": "https://github.com/sifex/stripe-connect",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alex",
            "email": "alex@platformapp.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*",
        "stripe/stripe-php": "^7.13.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Sifex\\StripeConnect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sifex\\StripeConnect\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sifex\\StripeConnect\\StripeConnectServiceProvider"
            ],
            "aliases": {
                "StripeConnect": "Sifex\\StripeConnect\\StripeConnectFacade"
            }
        }
    }
}
